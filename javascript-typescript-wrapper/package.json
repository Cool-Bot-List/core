{
  "name": "coolbotlist.js",
  "version": "2.0.0",
  "description": "A JavaScript/Typescript API wrapper for Cool Bot List.",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cool-Bot-List/Cool-Bot-List-API.git"
  },
  "author": "aspect and creeper",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cool-Bot-List/Cool-Bot-List-API/issues"
  },
  "homepage": "https://github.com/Cool-Bot-List/Cool-Bot-List-API#readme",
  "exports": {
    ".": [
      {
        "require": "./dist/index.js",
        "import": "./esm/discord.mjs"
      },
      "./dist/index.js"
    ],
    "./esm": "./esm/index.mjs"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "socket.io-client": "^2.3.0",
    "discord.js": "^12.3.1",
    "typescript": "^4.0.2",
    "ts-node": "^10.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^14.11.8",
    "@types/socket.io-client": "^1.4.33"
  }
}
